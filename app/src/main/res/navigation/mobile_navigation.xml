<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.varoo.firestore.home.HomeFragment"
        android:label="@string/title_home"
        tools:layout="@layout/home_fragment" />

    <fragment
        android:id="@+id/navigation_wishlist"
        android:name="com.varoo.firestore.wishlist.WishlistFragment"
        android:label="Wishlist"
        tools:layout="@layout/wishlist_fragment" />

    <fragment
        android:id="@+id/navigation_user"
        android:name="com.varoo.firestore.user.UserFragment"
        android:label="@string/title_user"
        tools:layout="@layout/user_fragment" />

</navigation>